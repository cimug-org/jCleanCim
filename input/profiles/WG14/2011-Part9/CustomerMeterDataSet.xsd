<xs:enumeration value="homePlug">
<xs:annotation/>
</xs:enumeration>
<xs:enumeration value="pager">
<xs:annotation/>
</xs:enumeration>
<xs:enumeration value="phone">
<xs:annotation/>
</xs:enumeration>
<xs:enumeration value="plc">
<xs:annotation/>
</xs:enumeration>
<xs:enumeration value="rf">
<xs:annotation/>
</xs:enumeration>
