			<xs:element name="Discrete" type="m:Discrete" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="DiscreteValue" type="m:DiscreteValue" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EnergyConsumer" type="m:EnergyConsumer" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="GeneratingUnit" type="m:GeneratingUnit" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="GeographicalRegion" type="m:GeographicalRegion" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="GrossToNetActivePowerCurve" type="m:GrossToNetActivePowerCurve" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="HydroPump" type="m:HydroPump" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="IEC61970CIMVersion" type="m:IEC61970CIMVersion" minOccurs="0" maxOccurs="unbounded"/>
