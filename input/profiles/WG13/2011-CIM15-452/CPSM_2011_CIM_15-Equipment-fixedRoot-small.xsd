			<xs:element name="MeasurementValueSource" type="m:MeasurementValueSource" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="OperationalLimitSet" type="m:OperationalLimitSet" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="OperationalLimitType" type="m:OperationalLimitType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PhaseTapChangerAsymetrical" type="m:PhaseTapChangerAsymetrical" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PhaseTapChangerLinear" type="m:PhaseTapChangerLinear" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PhaseTapChangerSymetrical" type="m:PhaseTapChangerSymetrical" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PhaseTapChangerTabular" type="m:PhaseTapChangerTabular" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PhaseTapChangerTabularPoint" type="m:PhaseTapChangerTabularPoint" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PowerTransformer" type="m:PowerTransformer" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="PowerTransformerEnd" type="m:PowerTransformerEnd" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RatioTapChanger" type="m:RatioTapChanger" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RatioTapChangerTabular" type="m:RatioTapChangerTabular" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="RatioTapChangerTabularPoint" type="m:RatioTapChangerTabularPoint" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="ReactiveCapabilityCurve" type="m:ReactiveCapabilityCurve" minOccurs="0" maxOccurs="unbounded"/>
